@{
  Layout = "_Layout";
}
@using DrSillyStringzFactory.Models;
<div class="container">
  <div class="jumbotron">
    <h3>Licenses</h3>
  </div>
  @if(Model.Count == 0)
  {
    <h6>There are no Licenses at this time.</h6>
  }
  <ol>
    @foreach(License license in Model)
    {
      <li>@Html.ActionLink(license.LicenseType,"Details",new {id = license.LicenseId})</li>
    }
  </ol>

  <p>@Html.ActionLink("Create License","Create")</p>
  <p>@Html.ActionLink("Home","Index","Home")</p>



</div>